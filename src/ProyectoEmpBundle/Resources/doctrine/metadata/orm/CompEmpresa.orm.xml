<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Compempresa" table="compempresa">
    <indexes>
      <index name="fk_compempresa_compusuario" columns="id_comp_usuarios"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="CIF" columns="CIF"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="compName" type="string" column="comp_name" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cif" type="string" column="CIF" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="antiguedad" type="string" column="antiguedad" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="Direccion" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombrePc" type="string" column="nombre_pc" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefonoPc" type="string" column="telefono_pc" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailPc" type="string" column="email_pc" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="compLogotipo" type="string" column="comp_logotipo" length="256" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idCompUsuarios" target-entity="Eusuarios" fetch="LAZY">
      <join-columns>
        <join-column name="id_comp_usuarios" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
